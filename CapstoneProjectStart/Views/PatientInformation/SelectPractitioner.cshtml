@model CapstoneProjectStart.Models.Practitioner

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Appointment</h4>
        <hr />
    <ol>
    <li>
        @Html.Label("Select Practitioner")
        <div class="col-md-10">
            @Html.DropDownList("Practioner", null, htmlAttributes: new { @class = "form-control" }))
            @Html.ValidationMessageFor(model => model.Last_Name, "", new { @class = "text-danger" })
    </div>
    </li>

</ol>

        <button>Select</button>
        </div>

}


